{
  "name": "uc-quantum-lab",
  "displayName": "UC Quantum Lab",
  "description": "An extension to make quantum development easier",
  "version": "0.1.4",
  "publisher": "UCQuantumLab",
  "author": {
    "name": "Marek Brodke",
    "email": "brodkemd@mail.uc.edu"
  },
  "qna": "https://github.com/UC-Advanced-Research-Computing/UC-Quantum-Lab/discussions",
  "repository": {
    "url": "https://github.com/UC-Advanced-Research-Computing/UC-Quantum-Lab",
    "type": "git"
  },
  "icon": "media/uc.png",
  "engines": {
    "vscode": "^1.71.0"
  },
  "bugs": {
    "url": "https://github.com/UC-Advanced-Research-Computing/UC-Quantum-Lab/issues",
    "email": "brodkemd@mail.uc.edu"
  },
  "categories": [
    "Data Science",
    "Visualization",
    "Education",
    "Testing",
    "Other"
  ],
  "keywords": [
    "quantum",
    "computing",
    "quantum computing",
    "qiksit",
    "circuit"
  ],
  "activationEvents": [
    "onCommand:uc-quantum-lab.execute",
    "onCommand:uc-quantum-lab.init"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "uc-quantum-lab.execute",
        "title": "execute",
        "shortTitle": "execute",
        "category": "UC Quantum",
        "icon": {
          "light": "media/uc_light.svg",
          "dark": "media/uc.svg"
        }
      },
      {
        "command": "uc-quantum-lab.init",
        "title": "init",
        "category": "UC Quantum"
      },
      {
        "command": "uc-quantum-lab.reinit",
        "title": "reinit",
        "category": "UC Quantum"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "when": "workspaceFolderCount >= 1 && resourceExtname == .py",
          "command": "uc-quantum-lab.execute",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "^16.18.11",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.24.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "extensionDependencies": [
    "ms-python.python"
  ],
  "dependencies": {
    "@vscode/vsce": "^2.16.0"
  }
}
